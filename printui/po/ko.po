#. LUM_IDS_MNTMSG_PHA_CPT_F_33
#: po/keystr.h:1559
msgid "Adjustment in Column F (-3 to +3)"
msgstr "F 열에서 조절(-3에서 +3)"

#. LUM_IDS_MNTMSG_PHA_CPT_G_33
#: po/keystr.h:1561
msgid "Adjustment in Column G (-3 to +3)"
msgstr "G 열에서 조절(-3에서 +3)"

#. LUM_IDS_MNTMSG_PHA_CPT_Q_44
